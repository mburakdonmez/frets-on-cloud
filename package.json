{
  "name": "frets-on-cloud",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "dev": "next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "typegen:supabase": "bunx supabase gen types typescript --project-id \"sbyxxegqzwqhxzibbtvx\" --schema public > src/database.types.ts && prettier --write \"src/database.types.ts\"",
    "typegen:prisma": "bunx prisma db pull && bunx prisma generate",
    "typegen": "bun run typegen:supabase && bun run typegen:prisma"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.832.0",
    "@aws-sdk/s3-request-presigner": "^3.832.0",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.10.1",
    "@supabase/ssr": "^0.6.1",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.69.0",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "@uidotdev/usehooks": "^2.4.1",
    "@uppy/aws-s3": "^4.2.3",
    "@uppy/core": "^4.4.6",
    "@uppy/drag-drop": "^4.1.3",
    "@uppy/file-input": "^4.1.3",
    "@uppy/progress-bar": "^4.2.1",
    "@uppy/react": "^4.3.0",
    "next": "^15.2.3",
    "prisma": "^6.10.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "zod": "^3.25.61"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.15",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  }
}